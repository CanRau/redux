{
  "name": "logux-redux",
  "version": "0.0.0",
  "description": "Redux compatible API for Logux",
  "keywords": [
    "logux",
    "client",
    "redux"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/logux-redux",
  "dependencies": {
    "logux-client": "logux/logux-client",
    "redux": "^3.7.1"
  },
  "devDependencies": {
    "docdash": "^0.4.0",
    "eslint": "^4.1.1",
    "eslint-config-logux": "^15.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-es5": "^1.1.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "jsdoc": "^3.4.3",
    "lint-staged": "^4.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "size-limit": "^0.3.0",
    "yaspeller-ci": "^0.4.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yarn docs && yaspeller-ci *.md docs/*.html",
    "clean": "rimraf docs/ coverage/ test/demo/build/",
    "lint": "eslint *.js test/{**/,}*.js",
    "docs": "jsdoc --configure .jsdocrc *.js",
    "test": "jest --coverage && yarn lint && yarn size && yarn spellcheck",
    "size": "size-limit 11KB"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-logux/browser"
  },
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
