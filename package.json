{
  "name": "logux-redux",
  "version": "0.1.7",
  "description": "Redux compatible API for Logux",
  "keywords": [
    "logux",
    "client",
    "redux"
  ],
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yarn docs && yaspeller-ci *.md docs/*.html",
    "clean": "rimraf docs/ coverage/",
    "lint": "eslint *.js test/{**/,}*.js",
    "docs": "jsdoc -d docs/ *.js",
    "test": "jest --coverage && yarn lint && size-limit && yarn spellcheck"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/logux-redux",
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "dependencies": {
    "logux-client": "logux/logux-client",
    "logux-core": "logux/logux-core",
    "nanoevents": "^1.0.5",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "create-react-class": "^15.6.3",
    "eslint": "^5.7.0",
    "eslint-config-logux": "^26.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.3.0",
    "nanodelay": "^0.1.4",
    "pre-commit": "^1.2.2",
    "react": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.2",
    "size-limit": "^0.20.1",
    "yaspeller-ci": "^1.0.0"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-logux/browser",
    "rules": {
      "es5/no-es6-static-methods": "off"
    }
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "12.8 KB"
    }
  ],
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "excludeFiles": [
      "docs/*.js.html"
    ],
    "dictionary": [
      "logux",
      "createStore",
      "docdash",
      "JSDoc",
      "js",
      "JS",
      "Versioning",
      "Redux",
      "UX",
      "preloadedState",
      "SemVer",
      "reconnection",
      "ws",
      "subprotocol",
      "getState",
      "CrossTabClient",
      "nextReducer",
      "replaceReducer",
      "storeCreator",
      "dispatchLocal",
      "dispatchCrossTab",
      "dispatchSync",
      "unsubscribe",
      "changeListener",
      "prevState",
      "IndexedDB",
      "IndexedStore",
      "isSubscribing",
      "createLoguxCreator",
      "LoguxStore",
      "reconnections",
      "GMT",
      "Mon",
      "Tue",
      "Thu",
      "Fri",
      "Oct"
    ]
  }
}
