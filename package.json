{
  "name": "@logux/redux",
  "version": "0.2.5",
  "description": "Redux compatible API for Logux",
  "keywords": [
    "logux",
    "client",
    "redux",
    "websocket"
  ],
  "scripts": {
    "api": "documentation build *.js -f md -o api.md",
    "spell": "yarn api && yaspeller-ci *.md",
    "clean": "rimraf api.md coverage/",
    "lint": "eslint *.js test/{**/,}*.js",
    "test": "jest --coverage && yarn lint && size-limit && yarn spell"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/redux",
  "peerDependencies": {
    "react": ">=16.8.4",
    "react-redux": ">=6.0.0"
  },
  "dependencies": {
    "@logux/client": "^0.3.2",
    "@logux/core": "^0.3.1",
    "nanoevents": "^2.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@logux/eslint-config": "^28.2.2",
    "clean-publish": "^1.1.2",
    "create-react-class": "^15.6.3",
    "documentation": "^11.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.4.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-import-helpers": "^1.0.1",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.0",
    "nanodelay": "^0.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "size-limit": "^1.3.6",
    "yaspeller-ci": "^1.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config/browser",
    "rules": {
      "es5/no-es6-static-methods": "off"
    }
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "12 KB"
    }
  ],
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ],
    "dictionary": [
      "changeListener",
      "createLoguxCreator",
      "createLoguxStore",
      "createStore",
      "CrossTabClient",
      "dispatchCrossTab",
      "dispatchLocal",
      "dispatchSync",
      "getState",
      "GraphQL",
      "Logux",
      "logux",
      "LoguxStore",
      "reconnection",
      "reconnections",
      "Redux",
      "replaceReducer",
      "SemVer",
      "storeCreator",
      "subprotocol",
      "unsubscribe",
      "Versioning",
      "ws"
    ]
  }
}
